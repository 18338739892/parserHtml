<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="userLogin" namespace="/" extends="web-default">
        <!--这个name是用到地址时使用如name!method.action-->
        <action name="userLoginAction" class="com.pkk.action.UserLoginAction">
            <result name="success">pages/main.jsp</result>
            <result name="fail">pages/error.jsp</result>
            <result name="test">pages/test.jsp</result>
        </action>
    </package>

</struts>